{
  "scripts": {
    "build": "rm -rf dist && NODE_ENV=production tsc",
    "ci": "npm run verify",
    "format": "prettier --write 'src/**/*.{json,md,ts}'",
    "format-verify": "prettier --check 'src/**/*.{json,md,ts}'",
    "prepublishOnly": "npm run verify && npm run build",
    "start-building": "rm -rf dist && NODE_ENV=production tsc --watch",
    "test": "ava",
    "typecheck": "tsc --noEmit",
    "verify": "npm run typecheck && npm run test && npm run format-verify"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.3.0",
    "typescript": "^3.7.5"
  },
  "devDependencies": {
    "@types/node": "^13.5.3",
    "@typescript-eslint/experimental-utils": "^2.17.0",
    "ava": "^3.1.0",
    "eslint": "^6.8.0",
    "prettier": "^2.0.2",
    "ts-node": "^8.6.2"
  },
  "description": "",
  "files": [
    "dist/**/*"
  ],
  "license": "ISC",
  "main": "dist/index.js",
  "name": "@theorem/eslint-plugin",
  "peerDependencies": {
    "eslint": "^6.8.0"
  },
  "repository": "git@github.com:Theorem/eslint-plugin.git",
  "sideEffects": false,
  "version": "2.0.1"
}
