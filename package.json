{
  "scripts": {
    "build": "rm -rf dist && NODE_ENV=production tsc",
    "ci": "npm run verify",
    "format": "prettier --write 'src/**/*.{js,json,md,ts}' '*.{js,json,md,ts}'",
    "format-verify": "prettier --check 'src/**/*.{json,md,ts}' '*.{js,json,md,ts}'",
    "prepublishOnly": "npm run verify && npm run build",
    "start-building": "rm -rf dist && NODE_ENV=production tsc --watch",
    "test": "ava",
    "typecheck": "tsc --noEmit",
    "verify": "npm run typecheck && npm run test && npm run format-verify"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "typescript": "^4.0.2"
  },
  "devDependencies": {
    "@types/node": "^14.6.0",
    "@typescript-eslint/experimental-utils": "^3.9.1",
    "ava": "^3.11.1",
    "eslint": "^7.7.0",
    "prettier": "^2.0.5",
    "ts-node": "^8.10.2"
  },
  "description": "",
  "files": [
    "dist/**/*"
  ],
  "license": "ISC",
  "main": "dist/index.js",
  "name": "@theorem/eslint-plugin",
  "peerDependencies": {
    "eslint": "^7.7.0"
  },
  "repository": "git@github.com:Theorem/eslint-plugin.git",
  "sideEffects": false,
  "version": "5.0.0"
}
